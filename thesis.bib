	pages = {717--720},
	file = {Schmidt et al. - 2001 - Prostate-specific antigen in female urine a prosp.pdf:C\:\\Users\\Dublin\\Zotero\\storage\\W6AFM9BC\\Schmidt et al. - 2001 - Prostate-specific antigen in female urine a prosp.pdf:application/pdf},
}

