@article{robertson_non-invasive_2020,
	title = {A non-invasive gating method for probing {2D} electron systems on pristine, intrinsic {H}-{Si}(111) surfaces},
	volume = {117},
	issn = {0003-6951, 1077-3118},
	url = {https://pubs.aip.org/apl/article/117/15/151603/39252/A-non-invasive-gating-method-for-probing-2D},
	doi = {10.1063/5.0024842},
	abstract = {Intrinsic Si(111) surfaces passivated with atomic hydrogen are an ideal platform to host two-dimensional electron systems. Traditional methods to probe these surfaces, however, typically involve the placement of dopants and metals directly onto the surface and subsequent high temperature processing, which can be harsh and invasive and lead to surface degradation. Here, we detail a non-invasive gating approach for probing two-dimensional electron systems on intrinsic H-Si(111) surfaces using a silicon-on-insulator (SOI) gating assembly. In this architecture, all harsh device fabrication is performed on a single SOI chip, ensuring that the H-Si(111) surface remains in pristine condition, or as close to the original manufactured intrinsic-Si wafer as possible. To achieve this, we intentionally keep our H-Si(111) surfaces free of any dopants or metals, which are instead placed on the adjacent SOI chip. All electrical components, including Ohmic contacts and accumulation and depletion gates, are housed in the SOI piece. The Ohmic contacts on the SOI piece are brought into physical and electrical contact with the pristine H-Si(111) piece after being van der Waals bonded at room temperature, while all gates on the SOI piece are separated from the H-Si(111) surface by vacuum. Architecture details, baseline operation tests, and 77 K device characterization measurements will be discussed, as well as the implications of going beyond H-Si(111) surfaces and using our device architecture to facilitate transport measurements on halogen-terminated Si surfaces.},
	language = {en},
	number = {15},
	urldate = {2024-03-20},
	journal = {Applied Physics Letters},
	author = {Robertson, L. D. and Kane, B. E.},
	month = oct,
	year = {2020},
	pages = {151603},
	file = {Robertson and Kane - 2020 - A non-invasive gating method for probing 2D electr.pdf:C\:\\Users\\Dublin\\Zotero\\storage\\ZURS2EHM\\Robertson and Kane - 2020 - A non-invasive gating method for probing 2D electr.pdf:application/pdf},
}

@article{zhao_acoustically_2020,
