	journal = {Applied Physics Letters},
	author = {Zheng, Jiangpo and Zhou, Jian and Zeng, Pei and Liu, Yi and Shen, Yiping and Yao, Wenze and Chen, Zhe and Wu, Jianhui and Xiong, Shuo and Chen, Yiqin and Shi, Xianglong and Liu, Jie and Fu, Yongqing and Duan, Huigao},
