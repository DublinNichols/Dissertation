	pages = {13839--13846},
	file = {Li et al. - 2021 - Dry Exfoliation of Large-Area 2D Monolayer and Het.pdf:C\:\\Users\\Dublin\\Zotero\\storage\\AY6IPEZZ\\Li et al. - 2021 - Dry Exfoliation of Large-Area 2D Monolayer and Het.pdf:application/pdf},
